<%= debug @value %>
  <thead>
    <tr>
      <th tabindex="0" rowspan="1" colspan="1">Intercept</th>
      <% @keys.each do |k| %>
        <th tabindex="0" rowspan="1" colspan="1"><%= k %></th>
      <% end %>
      <th tabindex="0" rowspan="1" colspan="1">U(A)</th>
      <th tabindex="0" rowspan="1" colspan="1">U(B)</th>
      <th tabindex="0" rowspan="1" colspan="1">U(C)</th>
      <th tabindex="0" rowspan="1" colspan="1">U(D)</th>
      <th tabindex="0" rowspan="1" colspan="1">U(E)</th>
      <th tabindex="0" rowspan="1" colspan="1">MÃ¡x Util</th>
      <th tabindex="0" rowspan="1" colspan="1">Logit</th>
      <th tabindex="0" rowspan="1" colspan="1">Logit(%)</th>
    </tr>
  </thead>

  <tbody>
